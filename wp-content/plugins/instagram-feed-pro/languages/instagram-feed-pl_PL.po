#: admin/SBI_Global_Settings.php:1037
msgid "GDPR"
msgstr "RODO"

#: admin/SBI_Global_Settings.php:1038
msgid "Automatic"
msgstr "Automatyczny"

#: admin/SBI_Global_Settings.php:1039
#: inc/admin/class-sbi-new-user.php:366
#: inc/admin/class-sbi-notifications.php:651
